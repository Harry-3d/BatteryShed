  <body>

    <nav class="navbar navbar-inverse navbar-fixed">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="#">Project name</a>
        </div>
        <div id="navbar" class="collapse navbar-collapse">
          <ul class="nav navbar-nav">
            <li class="active"><a href="#">Home</a></li>
            <li><a href="#about">About</a></li>
            <li><a href="#contact">Contact</a></li>
          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </nav>

    <div class="container">
      <div class="starter-template">
        <h1>{{title}}</h1>


        <table class="table">
            <tr><th>Object Name</th><th>Location in Museum</th><th>Location in Wild</th></tr>
            {{#results}}
            <tr><td>{{objectName}}</td><td>{{museumLocation}}</td><td>{{wildLocation}}</tr>
            {{/results}}
        </table>

        <h2>Add a new object </h2>
        <form method="POST" action="/">
          <div class="form-group">
            <label for="objectName">Object Name</label>
            <input type="text" class="form-control" id="objectName" name="objectName">
          </div>

          <div class="form-group">
            <label for="museumLocation">Location in Museum</label>
            <input type="text" class="form-control" id="museumLocation" name="museumLocation">
          </div>

            <div class="form-group">
            <label for="wildLocation">Location in Wild</label>
            <input type="text" class="form-control" id="wildLocation" name="wildLocation">
          </div>
          <button type="submit" class="btn btn-default">Submit</button>
        </form>
    </div><!-- /.container -->
    </body>
</html>